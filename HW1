library(ISLR)
data(College)
college <- read.csv("College.csv", header=T)

head(college[, 1:5])

rownames <- college[,1]
fix(college)
college <- college[,-1]
fix(college)

summary(college)
pairs(college[, 1:10])

boxplot(college$Outstate ~ college$Private, main = "Outstate",  xlab = "Private", ylab = "Outstate vs Private")

Elite <-rep("No", nrow(college))
Elite [college$Top10perc >50] <- "Yes"
Elite <-as.factor(Elite)
college <- data.frame(college, Elite)
fix(college)
summary(college$Elite)

boxplot(college$Elite, college$Outstate, xlab="Elite University", ylab="Out of State Tuition in USD", main="Outstate vs Elite")